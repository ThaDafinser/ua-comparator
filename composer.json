{
    "name": "mimmi20/ua-comparator",
    "description": "a little project to compare different useragent parser",
    "type": "library",
    "license": "MIT",
    "keywords": ["Browser", "user agent", "http", "parser", "user-agent"],
    "homepage": "https://github.com/mimmi20/ua-comparator",
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/mimmi20/ua-comparator/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/mimmi20/ua-comparator/issues",
        "source": "https://github.com/mimmi20/ua-comparator"
    },
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/mimmi20/uasparser.git"
        },
        {
            "type": "git",
            "url":  "https://github.com/browscap/browscap-php.git"
        }
    ],
    "require": {
        "php": ">= 5.5.0",
        "symfony/console": "~2",
        "composer/composer": "dev-master",
        "monolog/monolog": "~1",
        "mimmi20/browser-detector": "dev-master",
        "mimmi20/wurfl": "dev-extract-request",
        "mimmi20/wurflcache": "~1.3",
        "mimmi20/wurfl-data": "~1.6",
        "mimmi20/wurfl-generic-request": "dev-master",
        "ua-parser/uap-php": "dev-master",
        "browscap/browscap": "dev-master",
        "browscap/browscap-php": "3.x@dev",
        "crossjoin/browscap": "dev-master",
        "wurfl/wurfl-api": "~1.6",
        "piwik/device-detector": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "UaComparator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UaComparatorTest\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": ["UaComparator\\Tool\\ComposerHook::postInstall"],
        "post-update-cmd": ["UaComparator\\Tool\\ComposerHook::postUpdate"]
    }
}
